{
  "layer": "top",
  "position": "top",
  "exclusive": true,
  "fixed-center": true,
  "spacing": 15,
  "margin-right": 10,
  "modules-left": [
    "custom/logo",
    "sway/workspaces",
    "sway/mode"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right": [
    "mpris",
    "wireplumber",
    "custom/separator",
    "backlight",
    "custom/separator",
    "network#tun",
    "network#enp0s31f6",
    "network#enp34s0u1u4c2",
    "network#wlp0s20f3",
    "custom/separator",
    "battery"
  ],
  "custom/logo": {
    "format": "<span font='InputMono Nerd Font Complete'></span>"
  },
  "custom/separator": {
    "format": "|"
  },
  "sway/workspaces": {
    "format": "{icon}",
    "format-icons": {
      "1": "<span font='InputMono Nerd Font Complete'></span>",
      "2": "<span font='icomoon'>󰈹</span>",
      "3": "<span font='InputMono Nerd Font Complete'>󰒱</span>",
      "4": "<span font='icomoon'></span>",
      "brave": "<span font='icomoon'></span>",
      "default": "<span font='InputMono Nerd Font Complete'></span>"
    }
  },
  "backlight": {
    "device": "intel_backlight",
    "on-click": "ctl -b",
    "on-click-right": "ctl -B",
    "format": "<span font='InputMono Nerd Font Complete'>{icon} </span>{percent}",
    "format-icons": [
      "󰃚",
      "󰃛",
      "󰃜",
      "󰃝",
      "󰃞",
      "󰃟",
      "󰃠"
    ],
    "tooltip-format": "{percent}"
  },
  "mpris": {
    "player": "playerctld",
    "on-click": "ctl -t",
    "on-middle-click": "ctl -p",
    "on-click-middle": "ctl -p",
    "on-right-click": "ctl -T",
    "on-click-right": "ctl -T",
    "format": "<span font='InputMono Nerd Font Complete'>{player_icon} {status_icon} </span>{dynamic}",
    "format-stopped": "<span font='InputMono Nerd Font Complete'>{player_icon} {status_icon}</span>",
    "player-icons": {
      "default": "",
      "brave": "<span font='icomoon'></span>",
      "chromium": "<span font='icomoon'></span>",
      "firefox": "<span font='icomoon'>󰈹</span>",
      "tidal-hifi": "<span font='icomoon'></span>"
    },
    "status-icons": {
      "stopped": "󰓛",
      "playing": "󰐊",
      "paused": "󰏤"
    }
  },
  "wireplumber": {
    "on-click": "ctl -v",
    "on-click-middle": "ctl -m",
    "on-click-right": "ctl -V",
    "format": "<span font='InputMono Nerd Font Complete'>{icon} </span>{volume}",
    "format-muted": "<span font='InputMono Nerd Font Complete'>{icon} </span>󰖁</span>",
    "format-icons": [
      "󰕿",
      "󰖀",
      "󰕾"
    ],
    "tooltip-format": "{node_name}\n{volume}%"
  },
  "battery": {
    "states": {
      "p5": 5,
      "p10": 10,
      "p20": 20,
      "p30": 30,
      "p40": 40,
      "p50": 50,
      "p60": 60,
      "p70": 70,
      "p80": 80,
      "p90": 90,
      "p100": 100
    },
    "format-full": "<span font='InputMono Nerd Font Complete'>󱟢</span>",
    "format-discharging-p5":   "<span font='InputMono Nerd Font Complete'>󱃍 </span>{capacity}",
    "format-discharging-p10":  "<span font='InputMono Nerd Font Complete'>󰁺 </span>{capacity}",
    "format-discharging-p20":  "<span font='InputMono Nerd Font Complete'>󰁻 </span>{capacity}",
    "format-discharging-p30":  "<span font='InputMono Nerd Font Complete'>󰁼 </span>{capacity}",
    "format-discharging-p40":  "<span font='InputMono Nerd Font Complete'>󰁽 </span>{capacity}",
    "format-discharging-p50":  "<span font='InputMono Nerd Font Complete'>󰁾 </span>{capacity}",
    "format-discharging-p60":  "<span font='InputMono Nerd Font Complete'>󰁿 </span>{capacity}",
    "format-discharging-p70":  "<span font='InputMono Nerd Font Complete'>󰂀 </span>{capacity}",
    "format-discharging-p80":  "<span font='InputMono Nerd Font Complete'>󰂁 </span>{capacity}",
    "format-discharging-p90":  "<span font='InputMono Nerd Font Complete'>󰂂 </span>{capacity}",
    "format-discharging-p100": "<span font='InputMono Nerd Font Complete'>󰁹 </span>{capacity}",
    "format-charging-p5":   "<span font='InputMono Nerd Font Complete'>󰢟 </span>{capacity}",
    "format-charging-p10":  "<span font='InputMono Nerd Font Complete'>󰢜 </span>{capacity}",
    "format-charging-p20":  "<span font='InputMono Nerd Font Complete'>󰂆 </span>{capacity}",
    "format-charging-p30":  "<span font='InputMono Nerd Font Complete'>󰂇 </span>{capacity}",
    "format-charging-p40":  "<span font='InputMono Nerd Font Complete'>󰂈 </span>{capacity}",
    "format-charging-p50":  "<span font='InputMono Nerd Font Complete'>󰢝 </span>{capacity}",
    "format-charging-p60":  "<span font='InputMono Nerd Font Complete'>󰂉 </span>{capacity}",
    "format-charging-p70":  "<span font='InputMono Nerd Font Complete'>󰢞 </span>{capacity}",
    "format-charging-p80":  "<span font='InputMono Nerd Font Complete'>󰂊 </span>{capacity}",
    "format-charging-p90":  "<span font='InputMono Nerd Font Complete'>󰂋 </span>{capacity}",
    "format-charging-p100": "<span font='InputMono Nerd Font Complete'>󰂅 </span>{capacity}",
    "format-plugged-p5":   "<span font='InputMono Nerd Font Complete'>󱃍 </span>{capacity}",
    "format-plugged-p10":  "<span font='InputMono Nerd Font Complete'>󰁺 </span>{capacity}",
    "format-plugged-p20":  "<span font='InputMono Nerd Font Complete'>󰁻 </span>{capacity}",
    "format-plugged-p30":  "<span font='InputMono Nerd Font Complete'>󰁼 </span>{capacity}",
    "format-plugged-p40":  "<span font='InputMono Nerd Font Complete'>󰁽 </span>{capacity}",
    "format-plugged-p50":  "<span font='InputMono Nerd Font Complete'>󰁾 </span>{capacity}",
    "format-plugged-p60":  "<span font='InputMono Nerd Font Complete'>󰁿 </span>{capacity}",
    "format-plugged-p70":  "<span font='InputMono Nerd Font Complete'>󰂀 </span>{capacity}",
    "format-plugged-p80":  "<span font='InputMono Nerd Font Complete'>󰂁 </span>{capacity}",
    "format-plugged-p90":  "<span font='InputMono Nerd Font Complete'>󰂂 </span>{capacity}",
    "format-plugged-p100": "<span font='InputMono Nerd Font Complete'>󰁹 </span>{capacity}",
    "format": "<span font='InputMono Nerd Font Complete'>{icon} </span>{capacity}",
    "format-icons": [
      "󱃍",
      "󰁺",
      "󰁻",
      "󰁼",
      "󰁽",
      "󰁾",
      "󰁿",
      "󰂀",
      "󰂁",
      "󰂂",
      "󰁹"
    ],
    "tooltip-format": "{capacity}% {power}W\n{timeTo}"
  },
  "network#enp0s31f6": {
    "interface": "enp0s31f6",
    "format": "<span font='InputMono Nerd Font Complete'>󰈁</span>",
    "format-ethenet": "<span font='InputMono Nerd Font Complete'>󰈁</span>",
    "format-linked": "<span font='InputMono Nerd Font Complete'></span>",
    "format-disconnected": "",
    "format-disabled": "",
    "tooltip-format": "{ifname}\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-ethernet": "{ifname}\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-disconnected": "{ifname} disconnected",
    "tooltip-format-disabled": "{ifname} disabled",
    "on-click": "foot nmtui"
  },
  "network#enp34s0u1u4c2": {
    "interface": "enp34s0u1u4c2",
    "format": "<span font='InputMono Nerd Font Complete'>󰈁</span>",
    "format-ethenet": "<span font='InputMono Nerd Font Complete'>󰈁</span>",
    "format-linked": "<span font='InputMono Nerd Font Complete'></span>",
    "format-disconnected": "<span font='InputMono Nerd Font Complete'>󰈂</span>",
    "format-disabled": "",
    "tooltip-format": "{ifname}\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-ethernet": "{ifname}\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-disconnected": "{ifname} disconnected",
    "tooltip-format-disabled": "{ifname} disabled",
    "on-click": "foot nmtui"
  },
  "network#wlp0s20f3": {
    "interface": "wlp0s20f3",
    "format": "<span font='InputMono Nerd Font Complete'>{icon} </span>{signalStrength}",
    "format-wifi": "<span font='InputMono Nerd Font Complete'>{icon} </span>{signalStrength}",
    "format-linked": "<span font='InputMono Nerd Font Complete'></span>",
    "format-disconnected": "<span font='InputMono Nerd Font Complete'>󰤮</span>",
    "format-disabled": "<span font='InputMono Nerd Font Complete'>󰤮</span>",
    "format-icons": [
      "󰤯",
      "󰤟",
      "󰤢",
      "󰤥",
      "󰤨"
    ],
    "tooltip-format": "{ifname}: {signaldBm}dBm  {signalStrength}%\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-wifi": "{ifname}: {signaldBm}dBm  {signalStrength}%\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-disconnected": "{ifname} disconnected",
    "tooltip-format-disabled": "{ifname} disabled",
    "on-click": "foot nmtui"
  },
  "network#tun": {
    "interface": "tun*",
    "on-click": "swaymsg '[app_id=pulseUI] focus'",
    "format": "<span font='InputMono Nerd Font Complete'></span>",
    "format-disconnected": "",
    "format-disabled": "",
    "tooltip-format": "{ifname} connected",
    "tooltip-format-disconnected": "{ifname} disconnected",
    "tooltip-format-disabled": "{ifname} disabled"
  },
  "network#enp": {
    "interface": "enp*",
    "format": "<span font='InputMono Nerd Font Complete'>󰈁</span>",
    "format-ethenet": "<span font='InputMono Nerd Font Complete'>󰈁</span>",
    "format-linked": "<span font='InputMono Nerd Font Complete'></span>",
    "format-disconnected": "<span font='InputMono Nerd Font Complete'>󰈂</span>",
    "format-disabled": "",
    "tooltip-format": "{ifname}\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-ethernet": "{ifname}\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-disconnected": "{ifname} disconnected",
    "tooltip-format-disabled": "{ifname} disabled",
    "on-click": "foot nmtui"
  },
  "network#wlp": {
    "interface": "wlp*",
    "format": "<span font='InputMono Nerd Font Complete'>{icon} </span>{signalStrength}",
    "format-wifi": "<span font='InputMono Nerd Font Complete'>{icon} </span>{signalStrength}",
    "format-linked": "<span font='InputMono Nerd Font Complete'></span>",
    "format-disconnected": "<span font='InputMono Nerd Font Complete'>󰤮</span>",
    "format-disabled": "<span font='InputMono Nerd Font Complete'>󰤮</span>",
    "format-icons": [
      "󰤯",
      "󰤟",
      "󰤢",
      "󰤥",
      "󰤨"
    ],
    "tooltip-format": "{ifname}: {signaldBm}dBm  {signalStrength}%\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-wifi": "{ifname}: {signaldBm}dBm  {signalStrength}%\n{bandwidthDownBits}<span font='InputMono Nerd Font Complete'> </span>{bandwidthUpBits}<span font='InputMono Nerd Font Complete'></span>",
    "tooltip-format-disconnected": "{ifname} disconnected",
    "tooltip-format-disabled": "{ifname} disabled",
    "on-click": "foot nmtui"
  },
  "clock": {
    "format": "{:%H:%M}",
    "format-alt": "{:%A, %B %d, %Y (%R)}",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode"          : "year",
      "mode-mon-col"  : 3,
      "weeks-pos"     : "right",
      "on-scroll"     : 1,
      "on-click-right": "mode",
      "format": {
        "months":     "<span color='#D8DEE9'><b>{}</b></span>",
        "days":       "<span color='#ECEFF4'><b>{}</b></span>",
        "weeks":      "<span color='#D8DEE9'><b>W{}</b></span>",
        "weekdays":   "<span color='#5E81AC'><b>{}</b></span>",
        "today":      "<span color='#88C0D0'><b><u>{}</u></b></span>"
      }
    },
    "actions":  {
      "on-click-right": "mode"
    }
  }
}
